{
  "name": "sample-integration-angular",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "rm -rf ./node_modules/@angular-builders/custom-webpack/node_modules",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 4200",
    "build": "ng build --configuration production --base-href /sample-integration-angular/",
    "test": "ng test",
    "test:ci": "CHROME_BIN=chromium ng test --browsers=ChromeHeadlessCI --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "ng build --configuration production && serve -s dist/sample-integration-angular",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "private": true,
  "prettier": "@porsche-design-system/prettier-config",
  "dependencies": {
    "@angular/animations": "15.0.1",
    "@angular/common": "15.0.1",
    "@angular/compiler": "15.0.1",
    "@angular/core": "15.0.1",
    "@angular/forms": "15.0.1",
    "@angular/platform-browser": "15.0.1",
    "@angular/platform-browser-dynamic": "15.0.1",
    "@angular/router": "15.0.1",
    "@porsche-design-system/components-angular": "2.19.0-rc.2",
    "@porsche-design-system/utilities": "5.4.0",
    "@porschehn/navigation": "2.23.2",
    "rxjs": "^7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^14.1.0",
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular/cli": "15.0.0",
    "@angular/compiler-cli": "15.0.1",
    "@angular/language-service": "15.0.1",
    "@porsche-design-system/prettier-config": "^1.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^18.11.8",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "prettier": "^2.7.1",
    "protractor": "~7.0.0",
    "serve": "^14.0.1",
    "ts-node": "~10.9.1",
    "tslint": "~6.1.3",
    "typescript": "~4.8.4"
  }
}
